Algoritmo "somadosNumerico"
Var

cont, N, S: inteiro
//N de nº e S de soma

Inicio
      cont <- 1
      S <- 0
      Enquanto (cont <= 5) faca
      escreval ("Digite o ", cont, "º. valor : ")
      leia (N)
      S <- S + N
      cont <- cont + 1
      fimEnquanto
      
      escreval("A soma de todos os valores foi: ", S)


Fimalgoritmo